   "source": [
    "def count_bits(x: int) -> int:\n",
    "    \n",
    "    num_bits = 0\n",
    "    \n",
    "    while x:\n",
    "        num_bits += x & 1\n",
    "        x >>= 1\n",
    "    \n",
    "    return num_bits\n",
    "\n",
    "count_bits(5)"
   ]
